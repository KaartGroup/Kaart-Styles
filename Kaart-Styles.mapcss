/* Junctions */

/* Roundabouts */

way[highway=motorway][junction=roundabout] {
    width: 4;
    casing-width: 2;
    casing-color: #FB2B11;
    color: #CF2081;
    dashes: 15, 15;
}
way[highway=trunk][junction=roundabout] {
    width: 3.7;
    casing-width: 2;
    casing-color: #FB2B11;
    color: #DD2F22;
    dashes: 15, 15;
}
way[highway=primary][junction=roundabout] {
    z-index: 6;
    width: 3.1;
    casing-width: 2;
    casing-color: #FB2B11;
    color: #FFA500;
    dashes: 15, 15;
}
